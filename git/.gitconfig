[alias]
    set-user = !". D:/scoop/persist/git/git-set-user.sh" ""

[core]
    autocrlf = true
    excludesfile = "D:/scoop/persist/git/.gitignore_global"

[pretty]
    simple = %C(bold yellow)%h %C(bold red)%cn %Creset%s %C(cyan)%ar%Creset

[pull]
   rebase = true

[push]
   default = simple

[color "status"]
    added = green bold
    changed = yellow bold
    untracked = red bold
